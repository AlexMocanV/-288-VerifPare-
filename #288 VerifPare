#include <iostream>

using namespace std;

int main()
{
    int n,i,x[101],j,c,ok=1;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        cin>>x[i];
        if(x[i]%2==1)
        {
            cout<<"NU";
            ok=0;
            break;
        }
    }
    if(ok!=0)
    {
        cout<<"DA";
    }
    return 0;
}
